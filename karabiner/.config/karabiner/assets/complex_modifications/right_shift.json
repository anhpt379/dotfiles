{
  "title": "Open Mission Control with right_shift",
  "rules": [
    {
      "description": "Map right_shift to mission_control",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_shift",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "mission_control"
            }
          ]
        }
      ]
    }
  ]
}
