                "from": {
                  "key_code": "c",
                  "modifiers": {
                    "mandatory": ["right_control"]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a 'Google Chrome.app'"
                  }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "k",
                  "modifiers": {
                    "mandatory": ["right_control"]
                  }
