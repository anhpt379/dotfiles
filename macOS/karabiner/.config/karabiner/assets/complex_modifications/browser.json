{
  "title": "Browser apps",
  "rules": [
    {
      "description": "Browser: Focus on the address bar with COMMAND+O",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "key_code": "l",
              "modifiers": ["command"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.edgemac$",
                "^com\\.google\\.Chrome$",
                "^com\\.brave\\.Browser$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Browser: Copy URL in the address bar with COMMAND+Y",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "key_code": "l",
              "modifiers": ["command"]
            },
            {
              "key_code": "c",
              "modifiers": ["command"]
            },
            {
              "key_code": "escape"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.edgemac$",
                "^com\\.google\\.Chrome$",
                "^com\\.brave\\.Browser$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Browser: Accept suggestion & go with CONTROL+F",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.edgemac$",
                "^com\\.google\\.Chrome$",
                "^com\\.brave\\.Browser$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Browser: Move tab left/right (or up/down) with COMMAND+[/]",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "key_code": "page_down",
              "modifiers": ["control", "shift"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.edgemac$",
                "^com\\.google\\.Chrome$",
                "^com\\.brave\\.Browser$"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "key_code": "page_up",
              "modifiers": ["control", "shift"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.edgemac$",
                "^com\\.google\\.Chrome$",
                "^com\\.brave\\.Browser$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Browser: Search tabs with CONTROL+/",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "slash",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "a",
              "modifiers": ["command", "shift"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.edgemac$",
                "^com\\.google\\.Chrome$",
                "^com\\.brave\\.Browser$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Browser: Undo close tab with COMMAND+U",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "key_code": "t",
              "modifiers": ["command", "shift"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.edgemac$",
                "^com\\.google\\.Chrome$",
                "^com\\.brave\\.Browser$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Browser: Switch tabs with COMMAND+N/P",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": ["control"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.edgemac$",
                "^com\\.google\\.Chrome$",
                "^com\\.brave\\.Browser$"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": ["control", "shift"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.edgemac$",
                "^com\\.google\\.Chrome$",
                "^com\\.brave\\.Browser$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Browser: Switch to last used tab with COMMAND+` (requires CLUT extension)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "grave_accent_and_tilde",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "key_code": "w",
              "modifiers": ["option"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.edgemac$",
                "^com\\.google\\.Chrome$",
                "^com\\.brave\\.Browser$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Browser: Disable COMMAND+ENTER (I press them accidentally sometimes)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.edgemac$",
                "^com\\.google\\.Chrome$",
                "^com\\.brave\\.Browser$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
