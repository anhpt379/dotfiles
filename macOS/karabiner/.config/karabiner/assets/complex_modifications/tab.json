{
  "title": "TAB key",
  "rules": [
    {
      "description": "TAB: Switch windows of the same app with OPTION+TAB",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": ["option"]
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde",
              "modifiers": ["command"]
            }
          ]
        }
      ]
    }
  ]
}
