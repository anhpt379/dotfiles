{
  "title": "AltTab app",
  "rules": [
    {
      "description": "AltTab: COMMAND+CONTROL+N to switch to next window same app",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": ["command", "control"]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": ["option"]
            }
          ]
        }
      ]
    },
    {
      "description": "AltTab: COMMAND+CONTROL+P to switch to previous window same app",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": ["command", "control"]
            }
          },
          "to": [
            {
              "key_code": "p",
              "modifiers": ["option"]
            }
          ]
        }
      ]
    }
  ]
}
