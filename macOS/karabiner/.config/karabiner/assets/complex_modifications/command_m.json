{
  "title": "Disable COMMAND+M (minimize window)",
  "rules": [
    {
      "description": "Map COMMAND+M to vk_none",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "key_code": "vk_none"
            }
          ]
        }
      ]
    }
  ]
}

