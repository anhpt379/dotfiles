{
  "title": "Open macOS apps with COMMAND+1/2/3/4",
  "rules": [
    {
      "description": "Open Chrome with COMMAND+1",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "Open Slack with COMMAND+2",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "2",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Slack.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "Open Notes with COMMAND+3",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "3",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Notes.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "Open Kitty with COMMAND+4",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "4",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Kitty.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "Open SimpleClock with COMMAND+CAPSLOCK+T",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'SimpleClock.app'"
            }
          ]
        }
      ]
    }
  ]
}
