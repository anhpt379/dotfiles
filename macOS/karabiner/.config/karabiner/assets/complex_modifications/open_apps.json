{
  "title": "Open macOS apps with COMMAND+1/2/3/4/5",
  "rules": [
    {
      "description": "Open Brave with COMMAND+1",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Brave Browser.app' && open -g ~/Applications/MoveCursorToCenterOfActiveWindow.app"
            }
          ]
        }
      ]
    },
