{
  "title": "Open macOS apps with COMMAND+1/2/3/4/5",
  "rules": [
    {
      "description": "Open Finder with the DELETE (F12) key",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f12"
          },
          "to": [
            {
              "shell_command": "open -a Finder.app"
            }
          ]
        }
      ]
    },
    {
      "description": "Open Edge with COMMAND+1",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Microsoft Edge.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "Open Kitty with COMMAND+2",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "2",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "shell_command": "open -a Kitty.app"
            }
          ]
        }
      ]
    },
    {
      "description": "Open Slack with COMMAND+3",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "3",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "shell_command": "open -a Slack.app"
            }
          ]
        }
      ]
    },
    {
      "description": "Open Notes with COMMAND+4",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "4",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "shell_command": "open -a Notes.app"
            }
          ]
        }
      ]
    },
    {
      "description": "Open Zoom with COMMAND+5",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "5",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "shell_command": "open -a Zoom.us.app"
            }
          ]
        }
      ]
    }
  ]
}
